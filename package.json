    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react-native": "^0.378.0",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.2.0",
